<div class="container body-content">

    <h1>@ViewBag.title</h1>
    <h2 class="centered">Job results</h2>
 
 <table class="centeredTable">
       <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Employer</th>
            <th>Location</th>
            <th>PositionType</th>
            <th>Skills</th>

       </tr>
        @foreach (var job in ViewBag.jobs)
        {
            <tr>
                <td>@job.Id</td>
                <td>@job.Name</td>
                <td>@job.Employer</td>
                <td>@job.Location</td>
                <td>@job.PositionType</td>
                <td>@job.CoreCompetency</td>
            </tr>
        }
    </table>

</div>

