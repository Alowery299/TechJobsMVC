<div class="container body-content">

    <h1>@ViewBag.title</h1>
    <h2 class="centered">Job results</h2>
 
 <table class="centeredTable">
      
        @foreach (var job in ViewBag.jobs)
        {
            <tr>
                <td>
                   ID: @job.Id<br>
                   Name: @job.Name<br>
                   Employer: @job.Employer<br>
                   Location: @job.Location<br>
                   PositionType: @job.PositionType<br>
                   Skills: @job.CoreCompetency<br>
                </td>
            </tr>
            
        }
    </table>

</div>

